<h1>Sides#index</h1>
<p>Find me in app/views/sides/index.html.erb</p>

<%= link_to 'reload', game_sides_all_path, remote: true %>
<%= form_for @side, remote: true do |f| %>
    <%= f.text_field 'gamebuilding_id' %>
    <%= f.submit 'create' %>
<% end %>

<%= form_for @side, url: game_sides_update_path, method: :patch, remote: true do |f| %>
    <%= f.text_field 'gamebuilding_id' %>
    <%= f.text_field 'id' %>
    <%= f.submit 'update' %>
<% end %>

<%= form_for @side, url: game_sides_destroy_path, remote: true do |f| %>
    <%= f.text_field 'id' %>
    <%= f.submit 'delete' %>
<% end %>
<div id="sides"></div>
